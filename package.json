{
  "name": "recall-alpha-agent",
  "version": "0.1.1",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "build": "tsup src/index.ts --format esm --dts",
    "start": "tsc && node --loader ts-node/esm src/index.ts",
    "start:alpha": "tsc && node --loader ts-node/esm src/plugin-twitter-alpha/server.ts",
    "start:recall": "tsc && node --loader ts-node/esm src/plugin-recall-storage/server.ts",
    "start:all": "concurrently \"pnpm run start\" \"pnpm run start:alpha\" \"pnpm run start:recall\"",
    "dev": "nodemon --watch src --exec \"tsc && node --loader ts-node/esm src/index.ts\"",
    "dev:alpha": "nodemon --watch src/twitter-alpha --exec \"tsc && node --loader ts-node/esm src/plugin-twitter-alpha/server.ts\"",
    "dev:recall": "nodemon --watch src/plugin-recall-storage --exec \"tsc && node --loader ts-node/esm src/plugin-recall-storage/server.ts\"",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm run dev:alpha\" \"pnpm run dev:recall\"",
    "clean": "./scripts/clean.sh",
    "start:service:all": "pm2 start pnpm --name=\"all\" --restart-delay=3000 --max-restarts=10 -- run start:all",
    "stop:service:all": "pm2 stop all"
  },
  "dependencies": {
    "@ai-sdk/openai": "1.0.5",
    "@elizaos/adapter-postgres": "0.25.6-alpha.1",
    "@elizaos/adapter-sqlite": "0.1.9",
    "@elizaos/client-auto": "0.1.9",
    "@elizaos/client-direct": "0.1.9",
    "@elizaos/client-telegram": "0.1.9",
    "@elizaos/client-twitter": "0.1.9",
    "@elizaos/core": "^0.1.9",
    "@elizaos/plugin-bootstrap": "0.1.9",
    "@elizaos/plugin-image-generation": "0.1.9",
    "@elizaos/plugin-node": "0.1.9",
    "@elizaos/plugin-solana": "0.1.9",
    "@elizaos/plugin-starknet": "0.1.9",
    "@project-serum/serum": "^0.13.65",
    "@raydium-io/raydium-sdk-v2": "0.1.111-alpha",
    "@recallnet/chains": "^0.0.7",
    "@recallnet/sdk": "^0.0.8",
    "@solana/spl-token": "^0.4.12",
    "@solana/web3.js": "^1.98.0",
    "@tavily/core": "0.0.2",
    "@types/pg": "8.11.10",
    "agent-twitter-client": "^0.0.18",
    "ai": "3.4.33",
    "amqplib": "0.10.5",
    "axios": "^1.7.9",
    "better-sqlite3": "11.5.0",
    "bs58": "^6.0.0",
    "discord.js": "^14.16.1",
    "express": "4.21.1",
    "fs": "0.0.1-security",
    "langchain": "^0.0.84",
    "net": "1.0.2",
    "path": "0.12.7",
    "pg": "8.13.1",
    "puppeteer": "^24.1.1",
    "readline": "1.3.0",
    "sqlite-vec": "0.1.6",
    "twitter-api-v2": "^1.17.2",
    "url": "0.11.4",
    "uuid": "10.0.0",
    "viem": "^2.22.9",
    "ws": "8.18.0",
    "yargs": "17.7.2",
    "zod": "3.23.8"
  },
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
    "overrides": {
      "onnxruntime-node": "1.20.0"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/express": "5.0.0",
    "@types/multer": "^1.4.12",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "@typescript-eslint/utils": "^8.23.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "prettier": "^3.4.2",
    "ts-node": "10.9.2",
    "tsup": "8.3.5",
    "typescript": "5.5.3"
  }
}
